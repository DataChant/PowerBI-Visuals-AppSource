{
    "visual": {
        "name": "geoportalMap",
        "displayName": "Mapa Geoportal",
        "guid": "geoportalMap",
        "visualClassName": "Visual",
        "version": "2.0.4.2",
        "description": "Componente de Mapa do Geoportal.",
        "supportUrl": "suporte@geoportal.com.br",
        "gitHubUrl": ""
    },
    "author": {
        "name": "Ricardo de Souza",
        "email": "ricardo@geoportal.com.br"
    },
    "apiVersion": "5.11.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "supportsSynchronizingFilterState": true,
        "supportsLandingPage": true,
        "supportsEmptyDataView": true,
        "objects": {
            "configuracoes": {
                "properties": {
                    "ticket": {
                        "type": {
                            "text": true
                        }
                    }
                }
            }
        },
        "dataRoles": [
            {
                "displayName": "Regi\u00e3o",
                "name": "regiao",
                "kind": "Grouping"
            },
            {
                "name": "latitude",
                "kind": "Grouping",
                "displayName": "Latitude(y)"
            },
            {
                "name": "longitude",
                "kind": "Grouping",
                "displayName": "Longitude(x)"
            },
            {
                "name": "rotulo",
                "kind": "Grouping",
                "displayName": "R\u00f3tulo"
            }
        ],
        "dataViewMappings": [
            {
                "conditions": [
                    {
                        "regiao": {
                            "min": 0,
                            "max": 1
                        },
                        "latitude": {
                            "min": 0,
                            "max": 1
                        },
                        "longitude": {
                            "min": 0,
                            "max": 1
                        },
                        "rotulo": {
                            "min": 0,
                            "max": 1
                        }
                    }
                ],
                "table": {
                    "rows": {
                        "select": [
                            {
                                "bind": {
                                    "to": "regiao"
                                }
                            },
                            {
                                "bind": {
                                    "to": "latitude"
                                }
                            },
                            {
                                "bind": {
                                    "to": "longitude"
                                }
                            },
                            {
                                "bind": {
                                    "to": "rotulo"
                                }
                            }
                        ]
                    }
                }
            }
        ],
        "privileges": [
            {
                "name": "WebAccess",
                "essential": true,
                "parameters": [
                    "https://*.geoportal.com.br"
                ]
            }
        ]
    },
    "visualEntryPoint": "",
    "externalJS": [],
    "assets": {
        "icon": "assets/icon.png"
    }
}