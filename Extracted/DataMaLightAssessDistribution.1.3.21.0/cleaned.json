                        "select": [
                            {
                                "for": {
                                    "in": "comparison"
                                }
                            },
                            {
                                "for": {
                                    "in": "dimension"
                                }
                            },
                            {
                                "for": {
                                    "in": "metric"
                                }
                            }
                        ],
                        "dataReductionAlgorithm": {
                            "window": {
                                "count": 200000
                            }
                        }
                    }
                }
            }
