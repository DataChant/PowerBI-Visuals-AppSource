                "preferredTypes": [
                    {
                        "numeric": true
                    }
                ]
            }
        ],
        "objects": {
            "DataMaLightSettings": {
                "properties": {
                    "configuration": {
                        "type": {
                            "text": true
                        }
                    },
                    "edition": {
                        "type": {
                            "bool": true
                        }
                    }
                }
            }
        },
        "dataViewMappings": [
            {
                "conditions": [
                    {
                        "comparison": {
