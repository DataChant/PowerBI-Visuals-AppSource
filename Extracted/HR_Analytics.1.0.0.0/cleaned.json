{
    "visual": {
        "name": "HR_Analytics",
        "displayName": "Inside-Out Org Chart for HR Analytics",
        "guid": "HR_Analytics",
        "visualClassName": "Visual",
        "version": "1.0.0.0",
        "description": "Inside-Out Org Chart for HR Analytics shows organization hierarchy",
        "supportUrl": "https://innovationalofficesolution.com/power-bi-custom-charts",
        "gitHubUrl": "https://github.com/Narendar-OfficeSolution/financial-risk-opportunity-matrix"
    },
    "author": {
        "name": "Abhishek Singh",
        "email": "abhisheksingh@innovationalofficesolution.com"
    },
    "apiVersion": "5.3.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "dataRoles": [
            {
                "displayName": "Division",
                "name": "Division",
                "kind": "Measure"
            },
            {
                "displayName": "Sub-Division",
                "name": "Sub-Division",
                "kind": "Measure"
            },
            {
                "displayName": "Employee",
                "name": "Employeename",
                "kind": "Grouping"
            },
            {
                "displayName": "Experience",
                "name": "Experience",
                "kind": "Measure"
            },
            {
                "displayName": "Requirement",
                "name": "Requirement",
                "kind": "Measure"
            }
        ],
        "dataViewMappings": [
            {
                "categorical": {
                    "categories": {
                        "for": {
                            "in": "Employeename"
                        },
                        "dataReductionAlgorithm": {
                            "top": {}
                        }
                    },
                    "values": {
                        "select": [
                            {
                                "bind": {
                                    "to": "Division"
                                }
                            },
                            {
                                "bind": {
                                    "to": "Sub-Division"
                                }
                            },
                            {
                                "bind": {
                                    "to": "Experience"
                                }
                            },
                            {
                                "bind": {
                                    "to": "Requirement"
                                }
                            }
                        ]
                    }
                }
            }
        ],
        "objects": {
            "dataPoint": {
                "properties": {
                    "defaultColor": {
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    },
                    "showAllDataPoints": {
                        "type": {
                            "bool": true
                        }
                    },
                    "fill": {
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    },
                    "fillRule": {
                        "type": {
                            "fill": {}
                        }
                    },
                    "fontSize": {
                        "type": {
                            "formatting": {
                                "fontSize": true
                            }
                        }
                    }
                }
            }
        },
        "privileges": [
            {
                "name": "ExportContent",
                "essential": true
            }
        ]
    },
    "visualEntryPoint": "",
    "externalJS": [],
    "assets": {
        "icon": "assets/icon.png"
    }
}