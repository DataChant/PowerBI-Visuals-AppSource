{
    "visual": {
        "name": "DotPlotChartByOfficeSolution",
        "displayName": "Dot Plot Chart By Office Solution",
        "guid": "DotPlotChartByOfficeSolution",
        "visualClassName": "Visual",
        "version": "1.0.1.1",
        "description": "Quantile Dot Plot Chart provides a clear snapshot of a probability distribution by placing representative dots at selected quantile values. This simplified display makes it easy to grasp likely outcomes, offering an intuitive and accessible alternative to traditional continuous visualizations for understanding and interpreting probability intervals.",
        "supportUrl": "https://innovationalofficesolution.com/power-bi-custom-charts",
        "gitHubUrl": "https://github.com/Narendar-OfficeSolution"
    },
    "author": {
        "name": "Abhishek",
        "email": "abhisheksingh@innovationalofficesolution.com"
    },
    "apiVersion": "5.3.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "dataRoles": [
            {
                "displayName": "X axis(numeric)",
                "name": "category",
                "kind": "Grouping"
            },
            {
                "displayName": "Y axis(numeric)",
                "name": "measure",
                "kind": "Measure"
            },
            {
                "displayName": "Parameter(numeric)",
                "name": "parameter",
                "kind": "Grouping"
            }
        ],
        "objects": {
            "dropdown": {
                "displayName": "Dropdown Settings",
                "properties": {
                    "showDropdown": {
                        "displayName": "Show Dropdown",
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "dataPoint": {
                "properties": {
                    "showAllDataPoints": {
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "licenceKey": {
                "properties": {
                    "licenceValue": {
                        "type": {
                            "text": true
                        }
                    }
                }
            },
            "customLabel": {
                "properties": {
                    "lableXValue": {
                        "type": {
                            "text": true
                        }
                    },
                    "circleRepresentValue": {
                        "type": {
                            "text": true
                        }
                    },
                    "lableYValue": {
                        "type": {
                            "text": true
                        }
                    },
                    "fontSize": {
                        "type": {
                            "formatting": {
                                "fontSize": true
                            }
                        }
                    },
                    "defaultColor": {
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    },
                    "leftColor": {
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    },
                    "rightColor": {
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    }
                }
            }
        },
        "dataViewMappings": [
            {
                "conditions": [
                    {
                        "category": {
                            "max": 2
                        },
                        "measure": {
                            "max": 2
                        },
                        "parameter": {
                            "max": 1
                        }
                    }
                ],
                "categorical": {
                    "categories": {
                        "select": [
                            {
                                "for": {
                                    "in": "category"
                                }
                            },
                            {
                                "for": {
                                    "in": "parameter"
                                }
                            }
                        ],
                        "dataReductionAlgorithm": {
                            "top": {}
                        }
                    },
                    "values": {
                        "select": [
                            {
                                "for": {
                                    "in": "measure"
                                }
                            }
                        ]
                    }
                }
            }
        ],
        "tooltips": {
            "supportedTypes": {
                "default": true,
                "canvas": true
            },
            "roles": [
                "tooltips"
            ],
            "supportEnhancedTooltips": true
        },
        "privileges": []
    },
