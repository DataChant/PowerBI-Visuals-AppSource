{
    "visual": {
        "name": "APSmartXL",
        "displayName": "Acterys Smart XL 1.0.0.1",
        "guid": "APSmartXLStandard",
        "visualClassName": "Visual",
        "version": "1.0.0.1",
        "description": "The Acterys Smart XL visual for planning and analysis",
        "supportUrl": "https://knowledge.acterys.com/kb-tickets/new",
        "gitHubUrl": ""
    },
    "author": {
        "name": "Managility",
        "email": "martink@managlity.co"
    },
    "apiVersion": "5.8.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "supportsLandingPage": true,
        "supportsEmptyDataView": true,
        "dataRoles": [
            {
                "displayName": "PowerBI Fields",
                "name": "powerbiFields",
                "kind": "GroupingOrMeasure"
            },
            {
                "displayName": "Filter Measure",
                "name": "filterMeasure",
                "kind": "Measure"
            },
            {
                "displayName": "Current User",
                "name": "currentuser",
                "kind": "Measure"
            },
            {
                "displayName": "AP Principal",
                "name": "apPrincipal",
                "kind": "Measure"
            },
            {
                "displayName": "AP Key",
                "name": "apKey",
                "kind": "Measure"
            }
        ],
        "objects": {
            "advanced": {
                "displayName": "Advanced Settings",
                "properties": {
                    "xlMetadata": {
                        "displayName": "XL Metadata",
                        "type": {
                            "text": true
                        }
                    }
                }
            },
            "remoteOperations": {
                "displayName": "Remote Operations",
                "properties": {
                    "remoteFiltering": {
                        "displayName": "Show",
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "userPermissions": {
                "displayName": "User Permissions",
                "properties": {
                    "designerMode": {
                        "displayName": "Show",
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "chunkSettings": {
                "displayName": "Chunk Values",
                "properties": {
                    "writeChunkLimit": {
                        "displayName": "Write chunk limit (cells)",
                        "type": {
                            "numeric": true
                        }
                    },
                    "readChunkLimit": {
                        "displayName": "Read chunk limit (cells)",
                        "type": {
                            "numeric": true
                        }
                    },
                    "saveAsynchronous": {
                        "displayName": "Send save requests simultaneously",
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "apiDetails": {
                "displayName": "API Details",
                "properties": {
                    "baseUrl": {
                        "displayName": "API Base URL",
                        "type": {
                            "text": true
                        }
                    },
                    "databaseName": {
                        "displayName": "Tenant Name",
                        "type": {
                            "text": true
                        }
                    },
                    "authType": {
                        "displayName": "Auth Type",
                        "type": {
                            "enumeration": [
                                {
                                    "displayName": "Token Based",
                                    "value": "1"
                                },
                                {
                                    "displayName": "API Key",
                                    "value": "2"
                                },
                                {
                                    "displayName": "SSO",
                                    "value": "3"
                                },
                                {
                                    "displayName": "PBI AD Auth",
                                    "value": "4"
                                }
                            ]
                        }
                    }
                }
            }
        },
        "dataViewMappings": [
            {
                "conditions": [
                    {
                        "filterMeasure": {
                            "max": 1,
                            "min": 0
                        },
                        "powerbiFields": {
                            "min": 0
                        },
                        "currentuser": {
                            "max": 1,
                            "min": 0
                        },
                        "apPrincipal": {
                            "max": 1,
                            "min": 0
                        },
                        "apKey": {
                            "max": 1,
                            "min": 0
                        }
                    }
                ],
                "table": {
                    "rows": {
                        "select": [
                            {
                                "for": {
                                    "in": "powerbiFields"
                                }
                            },
                            {
                                "bind": {
                                    "to": "filterMeasure"
                                }
                            },
                            {
                                "bind": {
                                    "to": "currentuser"
                                }
                            },
                            {
                                "bind": {
                                    "to": "apPrincipal"
                                }
                            },
                            {
                                "bind": {
                                    "to": "apKey"
                                }
                            }
                        ],
                        "dataReductionAlgorithm": {
                            "top": {
                                "count": 30000
                            }
                        }
                    }
                }
            }
        ],
        "privileges": [
            {
                "name": "AADAuthentication",
                "parameters": [
                    "https://acterys.app/pbiauth"
                ]
            },
            {
                "name": "ExportContent",
                "essential": true
            },
            {
                "name": "LocalStorage",
                "essential": true
            },
            {
                "name": "WebAccess",
                "essential": true,
                "parameters": [
                    "*"
                ]
            }
        ]
    },
    "visualEntryPoint": "",
    "externalJS": [],
    "assets": {
        "icon": "assets/icon.png"
    }
}