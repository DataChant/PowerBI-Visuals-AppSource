{
    "visual": {
        "name": "pdfViewer",
        "displayName": "Pdf Viewer",
        "guid": "pdfViewer392A657C115F4015AD64793E71199A8B",
        "visualClassName": "Visual",
        "version": "1.2.2.6",
        "description": "This Visual utilizes PDF JS to display PDF documents from base64 encoded data",
        "supportUrl": "https://datadrivenideas.wordpress.com/",
        "gitHubUrl": "https://github.com/DanielSallander/Pdf-Viewer"
    },
    "author": {
        "name": "Daniel Sallander",
        "email": "daniel@dsanalytics.se"
    },
    "apiVersion": "5.1.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "privileges": [
            {
                "name": "ExportContent",
                "essential": true
            }
        ],
        "dataRoles": [
            {
                "displayName": "PDF Base64 Data",
                "name": "pdfData",
                "kind": "GroupingOrMeasure"
            },
            {
                "displayName": "PDF File Name",
                "name": "pdfFileName",
                "kind": "GroupingOrMeasure"
            },
            {
                "displayName": "Tooltip",
                "name": "tooltipData",
                "kind": "GroupingOrMeasure"
            }
        ],
        "objects": {
            "dataCard": {
                "properties": {
                    "showHeader": {
                        "type": {
                            "bool": true
                        }
                    },
                    "scrollOverflow": {
                        "type": {
                            "bool": true
                        }
                    },
                    "showExportButton": {
                        "type": {
                            "bool": true
                        }
                    }
                }
            }
        },
        "dataViewMappings": [
            {
                "conditions": [
                    {
                        "pdfData": {
                            "max": 1
                        },
                        "pdfFileName": {
                            "max": 1
                        },
                        "tooltipData": {
                            "max": 1
                        }
                    }
                ],
                "table": {
                    "rows": {
                        "select": [
                            {
                                "for": {
                                    "in": "pdfData"
                                }
                            },
                            {
                                "for": {
                                    "in": "pdfFileName"
                                }
                            },
                            {
                                "for": {
                                    "in": "tooltipData"
                                }
                            }
                        ]
                    },
                    "dataReductionAlgorithm": {
                        "top": {
                            "count": 2
                        }
                    }
                }
            }
        ],
        "supportsLandingPage": true,
        "supportsEmptyDataView": true
    },
    "visualEntryPoint": "",
    "externalJS": [],
    "assets": {
        "icon": "assets/icon.png"
    }
}