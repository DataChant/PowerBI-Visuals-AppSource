                            "text": true
                        }
                    },
                    "licenseKey": {
                        "displayName": "License key",
                        "description": "License key to activate all pro features",
                        "type": {
                            "text": true
                        }
                    },
                    "licenseStatus": {
                        "displayName": "License status",
                        "description": "Visual version",
                        "type": {
                            "text": true
                        }
                    }
                }
            },
            "switch": {
                "displayName": "Switch",
                "properties": {
                    "filteType": {
                        "displayName": "Filter (Pro)",
                        "description": "Set a filter for ON and OFF field values",
                        "type": {
                            "enumeration": [
                                {
                                    "displayNameKey": "Enum_exact",
                                    "displayName": "Exact match",
                                    "value": "exact"
                                },
                                {
                                    "displayNameKey": "Enum_includes",
                                    "displayName": "Includes (Pro)",
                                    "value": "includes"
                                },
                                {
                                    "displayNameKey": "Enum_startsWith",
                                    "displayName": "StartsWith (Pro)",
                                    "value": "startsWith"
                                },
                                {
                                    "displayNameKey": "Enum_endsWith",
                                    "displayName": "EndsWith (Pro)",
                                    "value": "endsWith"
                                }
                            ]
                        }
                    },
                    "turnOnValue": {
                        "displayName": "Value(s) for ON",
                        "description": "Set comma seperated values from data field to filter data when switch is turned on",
                        "type": {
                            "text": true
                        }
                    },
                    "turnOffValue": {
                        "displayName": "Value(s) for OFF",
                        "description": "Leave it blank if all values should be visible when switch is turned off",
                        "type": {
                            "text": true
                        }
                    },
                    "switchShape": {
                        "displayName": "Shape",
                        "description": "Display different shapes of switches",
                        "type": {
                            "enumeration": [
                                {
                                    "displayNameKey": "Enum_round",
                                    "displayName": "Round",
                                    "value": "round"
                                },
                                {
                                    "displayNameKey": "Enum_rectangle",
                                    "displayName": "Rectangle",
                                    "value": "rectangle"
                                }
                            ]
                        }
                    },
                    "onColor": {
                        "displayName": "Switch ON Color",
                        "description": "Sets bg color of switch, when turned on",
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    },
                    "offColor": {
                        "displayName": "Switch OFF Color",
                        "description": "Sets bg color of switch, when turned off",
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    },
                    "multiState": {
                        "displayName": "Multi state (Pro)",
