                        "type": {
                            "text": true
                        }
                    },
                    "offTitle": {
                        "displayName": "OFF title",
                        "description": "Set title for switch off state",
                        "type": {
                            "text": true
                        }
                    },
                    "unselectedTitle": {
                        "displayName": "Unselected title",
                        "description": "Set title for unseleted switch state",
                        "type": {
                            "text": true
                        }
                    },
                    "fontFamily": {
                        "displayName": "Font Family",
                        "type": {
                            "formatting": {
                                "fontFamily": true
                            }
                        }
                    },
                    "fontSize": {
                        "displayName": "Text Size",
                        "type": {
                            "formatting": {
                                "fontSize": true
                            }
                        }
                    },
                    "color": {
                        "displayName": "Color",
                        "description": "Sets color of title",
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    }
                }
            },
            "general": {
                "displayName": "General",
                "properties": {
                    "filter": {
                        "type": {
                            "filter": true
                        }
                    },
                    "selfFilter": {
                        "type": {
                            "filter": {
                                "selfFilter": false
                            }
                        }
                    }
                }
            }
        },
        "supportsHighlight": true,
        "suppressDefaultTitle": true,
        "supportsSynchronizingFilterState": true,
        "sorting": {
            "default": {}
        }
