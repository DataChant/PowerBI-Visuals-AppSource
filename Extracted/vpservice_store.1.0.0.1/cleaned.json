{
    "visual": {
        "name": "VPService",
        "displayName": "VP Service",
        "guid": "vpservice_store",
        "visualClassName": "Visual",
        "version": "1.0.0.1",
        "description": "The visual enables users to connect to the Power ON Visual Planner Service and write-back to various data sources from Power BI, including robust planning, data management and data collection capabilities.",
        "supportUrl": "vizsupport@poweronbi.com",
        "gitHubUrl": ""
    },
    "author": {
        "name": "Power ON",
        "email": "vizsupport@poweronbi.com"
    },
    "apiVersion": "3.8.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "dataRoles": [
            {
                "displayName": "Values",
                "name": "measure",
                "kind": "Measure"
            }
        ],
        "dataViewMappings": [
            {
                "categorical": {
                    "values": {
                        "select": [
                            {
                                "bind": {
                                    "to": "measure"
                                }
                            }
                        ]
                    }
                }
            }
        ],
        "suppressDefaultTitle": true,
        "objects": {
            "DataEntry": {
                "displayName": "Service",
                "properties": {
                    "WebServiceInternal": {
                        "displayName": "Service Address",
                        "type": {
                            "text": true
                        }
                    },
                    "initialRefresh": {
                        "displayName": "Initial Refresh",
                        "type": {
                            "bool": true
                        }
                    },
                    "initDelay": {
                        "displayName": "Init Delay",
                        "type": {
                            "numeric": true
                        }
                    },
                    "useAAD": {
                        "displayName": "Use SSO",
                        "type": {
                            "bool": true
                        }
                    }
                }
            }
        },
        "supportsLandingPage": true,
        "supportsEmptyDataView": true
    },
    "visualEntryPoint": "",
    "toolsVersion": null,
    "externalJS": [],
    "assets": {
        "icon": "assets/icon.png"
    }
}