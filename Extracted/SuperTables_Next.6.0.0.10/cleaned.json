{
    "visual": {
        "name": "SuperTables",
        "displayName": "SuperTables",
        "guid": "SuperTables_Next",
        "visualClassName": "SupertablesPBI",
        "version": "6.0.0.10",
        "description": "Supertables",
        "supportUrl": "https://appsforpowerbi.infotopics.com/contact-support",
        "gitHubUrl": ""
    },
    "author": {
        "name": "AppsForPowerBI",
        "email": "info@appsforpowerbi.infotopics.com"
    },
    "apiVersion": "5.11.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "dataRoles": [
            {
                "displayName": "Column",
                "name": "column",
                "kind": "Grouping"
            },
            {
                "displayName": "Value",
                "name": "value",
                "kind": "Measure"
            },
            {
                "displayName": "InputTables Username",
                "name": "username",
                "kind": "GroupingOrMeasure"
            }
        ],
        "objects": {
            "configSettings": {
                "displayName": "Configuration settings",
                "properties": {
                    "width": {
                        "type": {
                            "numeric": true
                        }
                    },
                    "height": {
                        "type": {
                            "numeric": true
                        }
                    }
                }
            },
            "settings": {
                "properties": {
                    "string": {
                        "type": {
                            "text": true
                        }
                    },
                    "state": {
                        "filterState": true,
                        "type": {
                            "text": true
                        }
                    },
                    "columnConfig": {
                        "type": {
                            "text": true
                        }
                    }
                }
            },
            "performance": {
                "displayName": "Performance",
                "properties": {
                    "allData": {
                        "displayName": "Remove data limiter",
                        "description": "Remove the 30.000 row limiter from the visual in reading view.",
                        "type": {
                            "bool": true
                        }
                    },
                    "allDevData": {
                        "displayName": "Remove dev data limiter",
                        "description": "Remove the 30.000 row limiter from the visual during the creation of the report.",
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "licensing": {
                "displayName": "Licensing",
                "properties": {
                    "key": {
                        "displayName": "Key",
                        "type": {
                            "text": true
                        }
                    },
                    "company": {
                        "displayName": "Company",
                        "type": {
                            "text": true
                        }
                    },
                    "expiryDate": {
                        "displayName": "Expiry date",
                        "type": {
                            "text": true
                        }
                    },
                    "nOfUsers": {
                        "displayName": "Number of users",
                        "type": {
                            "text": true
                        }
                    },
                    "licenseType": {
                        "displayName": "License type",
                        "type": {
                            "text": true
                        }
                    },
                    "companyMail": {
                        "displayName": "Company E-mail",
                        "type": {
                            "text": true
                        }
                    },
                    "extensions": {
                        "displayName": "Supported Extensions",
                        "type": {
                            "text": true
                        }
                    },
                    "reset": {
                        "displayName": "Reset",
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "grid": {
                "displayName": "Grid Settings",
                "properties": {
                    "key": {
                        "displayName": "Key",
                        "type": {
                            "text": true
                        }
                    },
                    "active": {
                        "displayName": "Active",
                        "type": {
                            "bool": true
                        }
                    },
                    "licensed": {
                        "displayName": "Licensed",
                        "type": {
                            "bool": true
                        }
                    },
                    "trial": {
                        "displayName": "Trial",
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "debug": {
                "properties": {
                    "show": {
                        "type": {
                            "bool": true
                        }
                    }
                }
            }
        },
        "dataViewMappings": [
            {
                "conditions": [
                    {
                        "username": {
                            "max": 1
                        }
                    }
                ],
                "table": {
                    "rows": {
                        "select": [
                            {
                                "for": {
                                    "in": "column"
                                }
                            },
                            {
                                "for": {
                                    "in": "value"
                                }
                            },
                            {
                                "for": {
                                    "in": "username"
                                }
                            }
                        ],
                        "dataReductionAlgorithm": {
                            "window": {
                                "count": 30000
                            }
                        }
                    }
                }
            }
        ],
        "supportsLandingPage": true,
        "supportsEmptyDataView": true,
        "suppressDefaultTitle": true,
        "privileges": [
            {
                "name": "ExportContent"
            },
            {
                "name": "WebAccess",
                "parameters": [
                    "*"
                ]
            },
            {
                "name": "LocalStorage",
                "essential": false
            }
        ]
    },
    "visualEntryPoint": "",
    "externalJS": [],
    "assets": {
        "icon": "assets/icon.png"
    }
}