    "stringResources": {},
    "capabilities": {
        "dataRoles": [
            {
                "displayName": "Y",
                "name": "variety",
                "kind": "Grouping"
            },
            {
                "displayName": "FY",
                "name": "site",
                "kind": "Grouping"
            },
            {
                "displayName": "X",
                "name": "yield",
                "kind": "Measure"
            },
            {
                "displayName": "Year",
                "name": "year",
                "kind": "Grouping"
            },
            {
                "displayName": "Probability",
                "name": "probability",
                "kind": "Measure"
            }
        ],
        "objects": {
            "dataPoint": {
                "properties": {
                    "defaultColor": {
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    },
                    "showAllDataPoints": {
                        "type": {
                            "bool": true
                        }
                    },
                    "fill": {
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    },
                    "fillRule": {
                        "type": {
                            "fill": {}
                        }
                    },
                    "fontSize": {
                        "type": {
                            "formatting": {
                                "fontSize": true
                            }
                        }
                    }
                }
            },
            "trellisChartSettings": {
                "properties": {
                    "showYAxis": {
                        "type": {
                            "bool": true
                        }
                    },
                    "showFYAxis": {
                        "type": {
                            "bool": true
                        }
                    },
                    "showXAxis": {
                        "type": {
                            "bool": true
                        }
                    },
                    "showYear": {
                        "type": {
                            "bool": true
                        }
                    },
                    "showProbability": {
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "licenseKey": {
                "properties": {
                    "license": {
                        "type": {
                            "text": true
                        }
                    }
                }
            }
        },
        "dataViewMappings": [
            {
                "conditions": [
                    {
                        "variety": {
                            "max": 3
                        },
                        "yield": {
                            "max": 3
                        },
                        "site": {
                            "max": 3
                        },
                        "year": {
                            "max": 3
                        },
                        "probability": {
                            "max": 3
                        }
                    }
                ],
                "categorical": {
                    "categories": {
                        "select": [
                            {
                                "for": {
                                    "in": "variety"
                                }
                            },
                            {
                                "for": {
                                    "in": "site"
                                }
                            },
                            {
                                "for": {
                                    "in": "year"
                                }
                            }
                        ]
                    },
                    "values": {
                        "select": [
                            {
                                "bind": {
                                    "to": "yield"
                                }
                            },
                            {
                                "bind": {
                                    "to": "probability"
                                }
                            }
                        ]
                    }
                }
            }
        ],
        "sorting": {
            "default": {}
        },
        "privileges": []
    },
