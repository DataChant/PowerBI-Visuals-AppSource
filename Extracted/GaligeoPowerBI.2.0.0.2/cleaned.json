{
    "visual": {
        "name": "GaligeoPowerBI",
        "displayName": "Galigeo for PowerBI (2.0.0.2)",
        "guid": "GaligeoPowerBI",
        "visualClassName": "Visual",
        "version": "2.0.0.2",
        "description": "Galigeo for PowerBI",
        "supportUrl": "https://galigeo.com",
        "gitHubUrl": "https://github.com/Galigeo/Galigeo-for-PowerBI"
    },
    "author": {
        "name": "Galigeo",
        "email": "contact@galigeo.com"
    },
    "apiVersion": "5.11.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "dataRoles": [
            {
                "displayName": "Category Data",
                "name": "category",
                "kind": "Grouping"
            },
            {
                "displayName": "Measure Data",
                "name": "measure",
                "kind": "Measure"
            }
        ],
        "objects": {
            "galigeo": {
                "displayName": "Galigeo Config",
                "properties": {
                    "url": {
                        "displayName": "Galigeo URL",
                        "description": "Specify the url to Galigeo. Ex: https://powerbi.galigeo.com/Galigeo",
                        "type": {
                            "text": true
                        }
                    },
                    "apiKey": {
                        "type": {
                            "text": true
                        }
                    },
                    "mapId": {
                        "type": {
                            "text": true
                        }
                    },
                    "highlight": {
                        "type": {
                            "bool": true
                        }
                    },
                    "infowindow": {
                        "type": {
                            "bool": true
                        }
                    }
                }
            }
        },
        "dataViewMappings": [
            {
                "categorical": {
                    "categories": {
                        "for": {
                            "in": "category"
                        },
                        "dataReductionAlgorithm": {
                            "top": {
                                "count": 30000
                            }
                        }
                    },
                    "values": {
                        "select": [
                            {
                                "bind": {
                                    "to": "measure"
                                }
                            }
                        ]
                    }
                }
            }
        ],
        "supportsHighlight": true,
        "privileges": [
            {
                "name": "WebAccess",
                "essential": true,
                "parameters": [
                    "*"
                ]
            },
            {
                "name": "ExportContent",
                "essential": true
            }
        ]
    },
    "visualEntryPoint": "",
    "externalJS": [],
    "assets": {
        "icon": "assets/icon.png"
    }
}