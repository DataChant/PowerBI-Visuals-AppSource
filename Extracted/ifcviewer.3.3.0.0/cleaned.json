{
    "visual": {
        "name": "ifcviewer",
        "displayName": "IFC Viewer",
        "guid": "ifcviewer",
        "visualClassName": "Visual",
        "version": "3.3.0.0",
        "description": "Visualize and analyze IFC models within Power BI",
        "supportUrl": "https://outlook.office365.com/book/SupportConsultingonlinemeeting@flinker.app",
        "gitHubUrl": ""
    },
    "author": {
        "name": "Flinker GmbH",
        "email": "info@flinker.app"
    },
    "apiVersion": "5.1.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "dataRoles": [
            {
                "name": "IfcChunks",
                "displayName": "Ifc Chunks",
                "kind": "Grouping"
            },
            {
                "name": "Ids",
                "displayName": "IDs",
                "kind": "Grouping"
            }
        ],
        "dataViewMappings": [
            {
                "categorical": {
                    "categories": {
                        "select": [
                            {
                                "for": {
                                    "in": "IfcChunks"
                                }
                            },
                            {
                                "bind": {
                                    "to": "Ids"
                                }
                            }
                        ],
                        "dataReductionAlgorithm": {
                            "window": {
                                "count": 30000
                            }
                        }
                    }
                }
            }
        ],
        "objects": {
            "colorSelector": {
                "displayName": "Element colors",
                "properties": {
                    "fill": {
                        "displayName": "Color",
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        },
                        "rule": {
                            "inputRole": "Ids",
                            "output": {
                                "property": "fill",
                                "selector": [
                                    "Ids"
                                ]
                            }
                        }
                    }
                }
            },
            "license": {
                "displayName": "License",
                "properties": {
                    "licenseKey": {
                        "displayName": "License key",
                        "type": {
                            "text": true
                        }
                    },
                    "licenseStore": {
                        "type": {
                            "text": true
                        }
                    }
                }
            }
        },
        "privileges": [
            {
                "essential": true,
                "name": "WebAccess",
                "parameters": [
                    "https://unpkg.com",
                    "https://viewer.flinker.app/",
                    "https://docs.flinker.app/",
                    "https://api.flinker.app/",
                    "https://api.simplesvg.com/",
                    "https://api.iconify.design/",
                    "https://forms.office.com/",
                    "https://localhost:8080/assets/visual.js.map",
                    "https://billing.stripe.com/",
                    "https://marketplace.microsoft.com/"
                ]
            },
            {
                "essential": false,
                "name": "ExportContent"
            },
            {
                "essential": true,
                "name": "LocalStorage",
                "parameters": []
            }
        ],
        "sorting": {
            "default": {}
        },
        "supportsEmptyDataView": true,
        "supportsKeyboardFocus": true,
        "supportsLandingPage": true,
        "keepAllMetadataColumns": true,
        "supportsMultiVisualSelection": true,
        "supportsSynchronizingFilterState": true,
        "suppressDefaultTitle": true,
        "tooltips": {
            "supportEnhancedTooltips": true
        }
    },
    "visualEntryPoint": "",
    "externalJS": [],
    "assets": {
        "icon": "assets/icon.png"
    }
}