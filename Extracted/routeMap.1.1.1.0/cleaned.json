{
    "visual": {
        "name": "route-map",
        "displayName": "Route Map",
        "guid": "routeMap",
        "visualClassName": "Visual",
        "version": "1.1.1.0",
        "description": "A visual displaying routes between locations on a map",
        "supportUrl": "www.lytiqa.se",
        "gitHubUrl": ""
    },
    "author": {
        "name": "John Hagelin",
        "email": "john@lytiqa.se"
    },
    "apiVersion": "5.3.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "privileges": [
            {
                "name": "WebAccess",
                "essential": true,
                "parameters": [
                    "https://*.openstreetmap.org",
                    "https://unpkg.com",
                    "https://tile.openstreetmap.org"
                ]
            }
        ],
        "dataRoles": [
            {
                "name": "origin",
                "kind": "Grouping",
                "displayName": "Origin",
                "requiredTypes": [
                    {
                        "text": true
                    }
                ]
            },
            {
                "name": "originLat",
                "kind": "GroupingOrMeasure",
                "displayName": "Origin Latitude",
                "requiredTypes": [
                    {
                        "numeric": true
                    }
                ]
            },
            {
                "name": "originLng",
                "kind": "GroupingOrMeasure",
                "displayName": "Origin Longitude",
                "requiredTypes": [
                    {
                        "numeric": true
                    }
                ]
            },
            {
                "name": "destination",
                "kind": "Grouping",
                "displayName": "Destination",
                "requiredTypes": [
                    {
                        "text": true
                    }
                ]
            },
            {
                "name": "destLat",
                "kind": "GroupingOrMeasure",
                "displayName": "Destination Latitude",
                "requiredTypes": [
                    {
                        "numeric": true
                    }
                ]
            },
            {
                "name": "destLng",
                "kind": "GroupingOrMeasure",
                "displayName": "Destination Longitude",
                "requiredTypes": [
                    {
                        "numeric": true
                    }
                ]
            },
            {
                "name": "legend",
                "kind": "Grouping",
                "displayName": "Legend"
            },
            {
                "name": "lineWidth",
                "kind": "Measure",
                "displayName": "Line Width",
                "requiredTypes": [
                    {
                        "numeric": true
                    }
                ]
            },
            {
                "name": "tooltips",
                "kind": "GroupingOrMeasure",
                "displayName": "Tooltips"
            }
        ],
        "objects": {
            "dataPoint": {
                "displayName": "Data colors",
                "properties": {
                    "defaultColor": {
                        "displayName": "Default color",
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    },
                    "fill": {
                        "displayName": "Fill",
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    },
                    "showAllDataPoints": {
                        "displayName": "Show all",
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "routeSettings": {
                "displayName": "Route Settings",
                "properties": {
                    "lineWidth": {
                        "displayName": "Line Width",
                        "type": {
                            "numeric": true
                        }
                    },
                    "lineColor": {
                        "displayName": "Line Color",
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    },
                    "bubbleSize": {
                        "displayName": "Bubble Size",
                        "type": {
                            "numeric": true
                        }
                    }
                }
            },
            "legend": {
                "displayName": "Legend",
                "properties": {
                    "show": {
                        "displayName": "Show",
                        "type": {
                            "bool": true
                        }
                    },
                    "position": {
                        "displayName": "Position",
                        "type": {
                            "enumeration": [
                                {
                                    "value": "Top",
                                    "displayName": "Top"
                                },
                                {
                                    "value": "Bottom",
                                    "displayName": "Bottom"
                                },
                                {
                                    "value": "Left",
                                    "displayName": "Left"
                                },
                                {
                                    "value": "Right",
                                    "displayName": "Right"
                                }
                            ]
                        }
                    },
                    "titleText": {
                        "displayName": "Title",
                        "type": {
                            "text": true
                        }
                    },
                    "fontSize": {
                        "displayName": "Text Size",
                        "type": {
                            "numeric": true
                        }
                    },
                    "fontFamily": {
                        "displayName": "Font family",
                        "type": {
                            "text": true
                        }
                    },
                    "showTitle": {
                        "displayName": "Title",
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "tooltips": {
                "displayName": "Tooltips",
                "description": "Tooltips settings",
                "properties": {}
            }
        },
        "dataViewMappings": [
            {
                "conditions": [
                    {
                        "origin": {
                            "max": 1
                        },
                        "originLat": {
                            "max": 1
                        },
                        "originLng": {
                            "max": 1
                        },
                        "destination": {
                            "max": 1
                        },
                        "destLat": {
                            "max": 1
                        },
                        "destLng": {
                            "max": 1
                        },
                        "legend": {
                            "max": 1
                        },
                        "lineWidth": {
                            "max": 1
                        }
                    }
                ],
                "categorical": {
                    "categories": {
                        "select": [
                            {
                                "for": {
                                    "in": "legend"
                                }
                            },
                            {
                                "for": {
                                    "in": "origin"
                                }
                            },
                            {
                                "for": {
                                    "in": "originLat"
                                }
                            },
                            {
                                "for": {
                                    "in": "originLng"
                                }
                            },
                            {
                                "for": {
                                    "in": "destination"
                                }
                            },
                            {
                                "for": {
                                    "in": "destLat"
                                }
                            },
                            {
                                "for": {
                                    "in": "destLng"
                                }
                            },
                            {
                                "for": {
                                    "in": "tooltips"
                                }
                            }
                        ]
                    },
                    "values": {
                        "select": [
                            {
                                "for": {
                                    "in": "lineWidth"
                                }
                            },
                            {
                                "bind": {
                                    "to": "tooltips"
                                }
                            }
                        ]
                    }
                }
            }
        ],
        "sorting": {
            "default": {}
        },
        "supportsHighlight": true,
        "supportsKeyboardFocus": true,
        "supportsLandingPage": true,
        "supportsMultiVisualSelection": true,
        "tooltips": {
            "supportedTypes": {
                "default": true,
                "canvas": true
            },
            "roles": [
                "tooltips"
            ],
            "supportEnhancedTooltips": true
        }
    },
    "visualEntryPoint": "",
    "externalJS": [],
    "assets": {
        "icon": "assets/icon.png"
    }
}