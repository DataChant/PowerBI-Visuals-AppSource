        "advancedEditModeSupport": 2,
        "supportsHighlight": true,
        "supportsLandingPage": false,
        "sorting": {
            "default": {}
        },
        "tooltips": {
            "supportedTypes": {
                "canvas": true,
                "default": true
            },
            "roles": [
                "tooltips"
            ],
            "supportEnhancedTooltips": true
        }
