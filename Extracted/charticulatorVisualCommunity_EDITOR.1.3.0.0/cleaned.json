                                {
                                    "displayName": "Giga (G)",
                                    "value": "giga",
                                    "displayNameKey": "Charticulator_Giga"
                                },
                                {
                                    "displayName": "Billions (B)",
                                    "value": "billions",
                                    "displayNameKey": "Charticulator_Billions"
