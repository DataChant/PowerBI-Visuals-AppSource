                            "enumeration": [
                                {
                                    "displayName": "Left",
                                    "value": "left",
                                    "displayNameKey": "Charticulator_PositionLeft"
                                },
                                {
                                    "displayName": "Right",
                                    "value": "right",
                                    "displayNameKey": "Charticulator_PositionRight"
                                }
