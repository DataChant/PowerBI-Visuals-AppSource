                "properties": {
                    "updateColors": {
                        "displayName": "Update colors",
                        "description": "Allow to update colors of scale on changing the theme of report",
                        "displayNameKey": "Charticulator_ColorSettings_UpdateColors",
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
