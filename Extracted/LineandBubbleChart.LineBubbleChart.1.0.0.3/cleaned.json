{
    "visual": {
        "name": "Line and BubbleChart",
        "displayName": "Line and BubbleChart",
        "guid": "LineBubbleChart",
        "visualClassName": "Visual",
        "version": "1.0.0.3",
        "description": "Show the line chart for grouped data by categories and data. The points on the chart will be dynamic bassed of data field provided",
        "supportUrl": "https://github.com/Narendar-OfficeSolution",
        "gitHubUrl": "https://github.com/Narendar-OfficeSolution"
    },
    "author": {
        "name": "MSOffice",
        "email": "Admin@innovationalofficesolution.com"
    },
    "apiVersion": "3.2.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "dataRoles": [
            {
                "displayName": "X Axis",
                "name": "xAxis",
                "kind": "Grouping"
            },
            {
                "displayName": "Y Axis",
                "name": "yAxis",
                "kind": "Measure"
            },
            {
                "displayName": "Size",
                "name": "size",
                "kind": "Measure"
            },
            {
                "displayName": "Legend",
                "name": "legend",
                "kind": "Grouping"
            },
            {
                "displayName": "Tooltip",
                "name": "tooltip",
                "kind": "Measure"
            }
        ],
        "objects": {
            "circle": {
                "displayName": "Y Scale Ratio",
                "properties": {
                    "max": {
                        "displayName": "Max %",
                        "description": "Extend the Y scale",
                        "type": {
                            "numeric": true
                        }
                    },
                    "min": {
                        "displayName": "Min %",
                        "description": "Extend the Y scale",
                        "type": {
                            "numeric": true
                        }
                    }
                }
            },
            "shapes": {
                "displayName": "Shapes",
                "properties": {
                    "markerShape": {
                        "displayName": "Marker shape",
                        "type": {
                            "enumeration": [
                                {
                                    "value": "circle",
                                    "displayName": "Circle"
                                },
                                {
                                    "value": "triangle",
                                    "displayName": "Triangle"
                                },
                                {
                                    "value": "diamond",
                                    "displayName": "Diamond"
                                },
                                {
                                    "value": "plus",
                                    "displayName": "Plus"
                                },
                                {
                                    "value": "square",
                                    "displayName": "Square"
                                },
                                {
                                    "value": "star",
                                    "displayName": "Star"
                                }
                            ]
                        }
                    }
                }
            },
            "numericalValueFormatter": {
                "displayName": "Numerical Value Formatter",
                "properties": {
                    "valueFormatter": {
                        "displayName": "Value Formatter",
                        "type": {
                            "enumeration": [
                                {
                                    "value": "auto",
                                    "displayName": "Auto"
                                },
                                {
                                    "value": "k",
                                    "displayName": "M"
                                },
                                {
                                    "value": "m",
                                    "displayName": "MM"
                                },
                                {
                                    "value": "b",
                                    "displayName": "B"
                                },
                                {
                                    "value": "t",
                                    "displayName": "T"
                                },
                                {
                                    "value": "none",
                                    "displayName": "None"
                                }
                            ]
                        }
                    }
                }
            },
            "axisLables": {
                "displayName": "Axis Labels",
                "properties": {
                    "xaxisLable": {
                        "displayName": "X Axis Label",
                        "type": {
                            "text": true
                        }
                    },
                    "yaxisLable": {
                        "displayName": "Y Axis Label",
                        "type": {
                            "text": true
                        }
                    }
                }
            },
            "licensing": {
                "displayName": "Licensing",
                "properties": {
                    "license": {
                        "displayName": "License Key",
                        "type": {
                            "text": true
                        }
                    }
                }
            },
            "visualStyle": {
                "displayName": "Visual Chart Style",
                "properties": {
                    "curveType": {
                        "displayName": "Curve Types",
                        "type": {
                            "enumeration": [
                                {
                                    "value": "curveLinear",
                                    "displayName": "Curve Linear"
                                },
                                {
                                    "value": "curveStep",
                                    "displayName": "Curve Step"
                                },
                                {
                                    "value": "curveStepBefore",
                                    "displayName": "Curve Step Before"
                                },
                                {
                                    "value": "curveStepAfter",
                                    "displayName": "Curve Step After"
                                },
                                {
                                    "value": "curveCardinal",
                                    "displayName": "Curve Cardinal"
                                },
                                {
                                    "value": "curveMonotoneX",
                                    "displayName": "Curve Monotone X"
                                },
                                {
                                    "value": "curveCatmullRom",
                                    "displayName": "Curve Catmull Rom"
                                }
                            ]
                        }
                    },
                    "lineType": {
                        "displayName": "Line Types",
                        "type": {
                            "enumeration": [
                                {
                                    "value": "solid",
                                    "displayName": "Solid"
                                },
                                {
                                    "value": "dotted",
                                    "displayName": "Dotted"
                                }
                            ]
                        }
                    },
                    "strokeWidth": {
                        "displayName": "Stroke Width",
                        "type": {
                            "text": true
                        }
                    },
                    "Opacity": {
                        "displayName": "Bubble Opacity",
                        "type": {
                            "text": true
                        }
                    },
                    "bubbleSize": {
                        "displayName": "Bubble Size",
                        "type": {
                            "text": true
                        }
                    }
                }
            },
            "colorSelector": {
                "displayName": "Data Colors",
                "properties": {
                    "fill": {
                        "displayName": "Color",
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    }
                }
            },
            "legend": {
                "displayName": "Legend",
                "properties": {
                    "enableLegend": {
                        "displayName": "Enable Legend",
                        "type": {
                            "bool": true
                        }
                    }
                }
            }
        },
        "tooltips": {
            "supportedTypes": {
                "default": true,
                "canvas": true
            },
            "roles": [
                "Tooltips"
            ],
            "supportEnhancedTooltips": true
        },
        "sorting": {
            "default": {}
        },
        "dataViewMappings": [
            {
                "conditions": [
                    {
                        "xAxis": {
                            "max": 1
                        },
                        "yAxis": {
                            "max": 1
                        },
                        "size": {
                            "max": 1
                        },
                        "legend": {
                            "max": 1
                        },
                        "tooltip": {
                            "max": 4
                        }
                    }
                ],
                "categorical": {
                    "categories": {
                        "select": [
                            {
                                "bind": {
                                    "to": "xAxis"
                                }
                            },
                            {
                                "bind": {
                                    "to": "legend"
                                }
                            }
                        ]
                    },
                    "values": {
                        "select": [
                            {
                                "bind": {
                                    "to": "yAxis"
                                }
                            },
                            {
                                "bind": {
                                    "to": "size"
                                }
                            },
                            {
                                "bind": {
                                    "to": "tooltip"
                                }
                            }
                        ]
                    }
                }
            }
        ]
    },
    "visualEntryPoint": "",
    "toolsVersion": null,
    "externalJS": [],
    "assets": {
        "icon": "assets/icon.png"
    }
