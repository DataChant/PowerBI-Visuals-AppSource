    },
    "apiVersion": "5.3.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "dataRoles": [
            {
                "displayName": "Category Data",
                "name": "category",
                "kind": "Grouping"
            },
            {
                "displayName": "Measure Data (y)",
                "name": "measure",
                "kind": "Measure"
            },
            {
                "displayName": "Expected Data (Y')",
                "name": "expected",
                "kind": "Measure"
            },
            {
                "displayName": "LEGENDS",
                "name": "legend",
                "kind": "Grouping"
            }
        ],
        "objects": {
            "abcd": {
                "properties": {
                    "barColor": {
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    },
                    "normalcurve": {
                        "type": {
                            "bool": true
                        }
                    },
                    "normalheight": {
                        "type": {
                            "bool": true
                        }
                    },
                    "actualheight": {
                        "type": {
                            "bool": true
                        }
                    },
                    "star": {
                        "type": {
                            "bool": true
                        }
                    },
                    "dotcircle": {
                        "type": {
                            "bool": true
                        }
                    },
                    "tcharts": {
                        "type": {
                            "bool": true
                        }
                    },
                    "Xaxis": {
                        "type": {
                            "bool": true
                        }
                    },
                    "Observed": {
                        "type": {
                            "bool": true
                        }
                    },
                    "Expected": {
                        "type": {
                            "bool": true
                        }
                    },
                    "SLegend": {
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "LicenseKey": {
                "properties": {
                    "license": {
                        "type": {
                            "text": true
                        }
                    }
                }
            }
        },
        "dataViewMappings": [
            {
                "categorical": {
                    "categories": {
                        "select": [
                            {
                                "for": {
                                    "in": "category"
                                }
                            },
                            {
                                "for": {
                                    "in": "legend"
                                }
                            }
                        ],
                        "dataReductionAlgorithm": {
                            "top": {}
                        }
                    },
                    "values": {
                        "select": [
                            {
                                "bind": {
                                    "to": "measure"
                                }
                            },
                            {
                                "bind": {
                                    "to": "expected"
                                }
                            }
                        ]
                    }
                }
            }
        ],
        "privileges": []
    },
