    "capabilities": {
        "dataRoles": [
            {
                "displayName": "Date Source",
                "name": "date",
                "kind": "Grouping",
                "description": "Provide a value that is a date that corresponds to events on the timeline.",
                "preferredTypes": [
                    {
                        "numeric": true
                    }
                ]
            },
            {
                "displayName": "Milestone Text",
                "name": "milestone",
                "kind": "Grouping",
                "description": "Provide a text description of the event to include on the timeline.",
                "preferredTypes": [
                    {
                        "text": true
                    }
                ]
            },
            {
                "displayName": "Milestone Color (optional)",
                "name": "mscolor",
                "kind": "GroupingOrMeasure",
                "description": "Optionally name a color (known color or #hex notation) that applies to the corresponding timeline event.",
                "preferredTypes": [
                    {
                        "text": true
                    }
                ]
            },
            {
                "displayName": "End Date (optional)",
                "name": "enddate",
                "kind": "GroupingOrMeasure",
                "description": "Optionally provide an end date for the corresponding timeline event; will extend the size of the event marker. Can be 'today' to extend to the current date.",
                "preferredTypes": [
                    {
                        "numeric": true
                    },
                    {
                        "text": true
                    }
                ]
            },
            {
                "displayName": "Image Source (optional)",
                "name": "imgsource",
                "kind": "GroupingOrMeasure",
                "description": "Optionally a text base64 encoded representation of an image to include alongside the event. See tools such as https://www.base64-image.de to convert an image to base64 text.",
                "preferredTypes": [
                    {
                        "text": true
                    }
                ]
            },
            {
                "displayName": "Category (optional)",
                "name": "category",
                "kind": "GroupingOrMeasure",
                "description": "Optionally a category name for the event.",
                "preferredTypes": [
                    {
                        "text": true
                    }
                ]
            },
            {
                "displayName": "Category Color (optional)",
                "name": "catcolor",
                "kind": "GroupingOrMeasure",
                "description": "Optionally a category color for the event.",
                "preferredTypes": [
                    {
                        "text": true
                    }
                ]
            },
            {
                "displayName": "Summary Level (optional)",
                "name": "sumlevel",
                "kind": "Measure",
                "description": "Optionally control the aggregation level for the visual using data (eg. via a slicer) versus the default setting.",
                "preferredTypes": [
                    {
                        "text": true
                    }
                ]
            }
        ],
