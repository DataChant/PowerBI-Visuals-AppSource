    "visual": {
        "name": "timeline",
        "displayName": "Timeline (by BI-Champ)",
        "guid": "timeline067F12826E684F83B734273E464AE6A3",
        "visualClassName": "Visual",
        "version": "1.4.0.0",
        "description": "Create a timeline with dots representing events, keyed by dates.",
        "supportUrl": "https://bi-champ.com/support",
        "gitHubUrl": ""
    },
    "author": {
        "name": "CodeX Enterprises LLC",
