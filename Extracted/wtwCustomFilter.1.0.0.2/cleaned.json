{
    "visual": {
        "name": "wtwCustomFilter",
        "displayName": "WTW Custom Filter",
        "guid": "wtwCustomFilter",
        "visualClassName": "Visual",
        "version": "1.0.0.2",
        "description": "A custom Power BI visual that provides a numeric input slicer allowing users to filter data by entering a number manually.",
        "supportUrl": "https://www.areyounet.com/runet/client/Reponse?i=707814012&s=898D64E8",
        "gitHubUrl": "https://github.com/wtw-hbna/WTW_Custom_Filter"
    },
    "author": {
        "name": "WTW France",
        "email": "data_hb_fr@wtwco.com"
    },
    "apiVersion": "5.3.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "dataRoles": [
            {
                "displayName": "Values",
                "name": "category",
                "kind": "Grouping"
            }
        ],
        "dataViewMappings": [
            {
                "conditions": [
                    {
                        "category": {
                            "max": 1
                        }
                    }
                ],
                "categorical": {
                    "categories": {
                        "for": {
                            "in": "category"
                        }
                    }
                }
            }
        ],
        "objects": {
            "general": {
                "displayName": "General",
                "properties": {
                    "filter": {
                        "type": {
                            "filter": {}
                        }
                    }
                }
            },
            "selection": {
                "displayName": "Selection",
                "properties": {
                    "singleSelect": {
                        "displayName": "Single Select",
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "slicerSettingsCard": {
                "displayName": "Slicer Settings",
                "properties": {
                    "fontSize": {
                        "displayName": "Font Size",
                        "type": {
                            "numeric": true
                        }
                    }
                }
            }
        },
        "supportsHighlight": true,
        "supportsSynchronizingFilterState": true,
        "supportsEmptyDataView": true,
        "sorting": {
            "default": {}
        },
        "drilldown": {
            "roles": [
                "category"
            ]
        },
        "privileges": []
    },
    "visualEntryPoint": "",
    "externalJS": [],
    "assets": {
        "icon": "assets/icon.png"
    }
}