{
    "visual": {
        "name": "myCustomVisual",
        "displayName": "My Custom Visual",
        "guid": "myCustomVisual12345",
        "visualClassName": "Visual",
        "version": "1.0.0.4",
        "description": "A simple Power BI custom visual for testing constructor and update events",
        "supportUrl": "https://learn.microsoft.com/power-bi/developer/",
        "gitHubUrl": ""
    },
    "author": {
        "name": "Faizan",
        "email": "faizan@example.com"
    },
    "apiVersion": "5.11.0",
    "style": "style/visual.less",
    "stringResources": {},
    "capabilities": {
        "dataRoles": [
            {
                "name": "category",
                "kind": "Grouping",
                "displayName": "Category"
            },
            {
                "name": "value",
                "kind": "Measure",
                "displayName": "Value"
            }
        ],
        "dataViewMappings": [
            {
                "categorical": {
                    "categories": {
                        "for": {
                            "in": "category"
                        },
                        "dataReductionAlgorithm": {
                            "top": {}
                        }
                    },
                    "values": {
                        "select": [
                            {
                                "for": {
                                    "in": "value"
                                }
                            }
                        ]
                    }
                }
            }
        ],
        "objects": {
            "general": {
                "displayName": "General",
                "properties": {
                    "barHeight": {
                        "type": {
                            "numeric": true
                        }
                    },
                    "orientation": {
                        "type": {
                            "text": true
                        }
                    },
                    "showScroll": {
                        "type": {
                            "bool": true
                        }
                    }
                }
            },
            "dataColors": {
                "displayName": "Data Colors",
                "properties": {
                    "fill": {
                        "type": {
                            "fill": {
                                "solid": {
                                    "color": true
                                }
                            }
                        }
                    }
                }
            },
            "tooltips": {
                "displayName": "Tooltips",
                "properties": {
                    "showTooltips": {
                        "type": {
                            "bool": true
                        }
                    },
                    "tooltipFontSize": {
                        "type": {
                            "numeric": true
                        }
                    }
                }
            },
            "animation": {
                "displayName": "Animation",
                "properties": {
                    "enableAnimation": {
                        "type": {
                            "bool": true
                        }
                    },
                    "animationSpeed": {
                        "type": {
                            "numeric": true
                        }
                    }
                }
            }
        },
        "supportsHighlight": true,
        "suppressDefaultTitle": true,
        "privileges": []
    },
    "visualEntryPoint": "",
    "externalJS": [],
    "assets": {
        "icon": "assets/icon.png"
    }
}